include "Constant.fbs";

namespace GenericBoson.Zozo;

table LoginReq
{
    account: string;
    password: string;
}

table LoginAck
{
    result_code: ResultCode;
    gameserverip: string;
    gameserverport: string;
    token: string;
}

union LobbyPayload
{
    LoginReq,
    LoginAck,
}

table LobbyMessage
{
    payload:LobbyPayload;
}

root_type LoginReq;
root_type LoginAck;
root_type LobbyMessage;