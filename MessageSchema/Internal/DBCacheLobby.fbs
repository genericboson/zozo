include "InternalConstant.fbs";

namespace GenericBoson.Zozo;

table LoginDBReq
{
    account: string;
    password: string;
}

table LoginDBAck
{
    result_code: InternalResultCode;
    gameserver_ip: string;
    gameserver_port: string;
    token: string;
}

union DBCacheLobbyPayload
{
    LoginDBReq,
    LoginDBAck,
}

table DBCacheLobbyMessage
{
    payload:DBCacheLobbyPayload;
}

root_type LoginDBReq;
root_type LoginDBAck;
root_type DBCacheLobbyMessage;