include "Constant.fbs";
include "CharacterInfo.fbs";
include "ServerInfo.fbs";

namespace GenericBoson.Zozo;

table LoginDBReq
{
    account: string;
}

table LoginDBAck
{
    result_code : ResultCode;
    server_infos: [ServerInfo];
    characters  : [CharacterInfo];
    password    : string;
}

union DBCacheLobbyPayload
{
    LoginDBReq,
    LoginDBAck,
}

table DBCacheLobbyMessage
{
    payload:DBCacheLobbyPayload;
}

root_type LoginDBReq;
root_type LoginDBAck;
root_type DBCacheLobbyMessage;